package it.akt;

public class Rubrica {

	private Contatto [] contatti = new Contatto [20];
	 
	public Rubrica() {
		for (int i = 0; i < contatti.length ; i++) {
			contatti[i] = new Contatto ("Nome " + (i + 1), "Cognome " + (i + 1) , "224466 " + (100 + i));
	
		}
	}
	public void stampaRubrica() {
		for (int i = 0 ; i < contatti.length ; i++) {
		 contatti[i].stampaContatto();
		}
	}
 
	public Contatto[] getContatti() {
		return contatti; 
	
	
	}
	
 
	
}
